<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragment.lembaga.FragmentLembaga">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarLembagaDesa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/green">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="@string/lembaga_desa_cibentang"
            android:textAppearance="@style/custom_text_toolbar"
            android:textColor="@color/white" />

    </androidx.appcompat.widget.Toolbar>


    <include
        android:id="@+id/layout_lembaga_desa"
        layout="@layout/layout_lembaga_desa"/>

</LinearLayout>